<h1>Editing User</h1>


<%= form_for @user do |f| %>

<%= f.label "Email" %>
<%= f.text_field :email %>
<%= f.label "Password" %>
<%= f.password_field :password %>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<%= f.label "Admin?" %>
<%= f.check_box :admin %>
<%= f.submit "Edit" %>
<% end %>



<%= link_to 'Show', @user %> |
<%= link_to 'Back', users_path %>
